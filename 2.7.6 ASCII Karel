function main() {
    // Call your functions here
    printKarel();
    printBall();
    
}

// Define your functions here
function printKarel(){
    console.log("   ()_____");
    console.log("  //     O)___");
    console.log(" //           )");
    console.log("//      _____/");
}







function printBall() {
   console.log("  .-''-. ");
   console.log(" /      \\");
   console.log("|  () () |");
   console.log(" \\      /");
   console.log("  '-..-' ");
}








main();
