/* This program draws a big tower from Karel's starting spot */
function main() {
	// Write your code here
	north();
	empty();
	putBall();
}

function empty (){
    while (frontIsClear()) {
          putBall();
          move();
       }
}

function north (){
    while (notFacingNorth()) {
          turnLeft();
      }
}
main();
