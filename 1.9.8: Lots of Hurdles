function  main () {
     for (let i = 0; i < 5; i ++) {
           runToHurdle();
         jumpHurdle();

       }
}


 function runToHurdle() {
       move();
       move();
}

function jumpHurdle() {
     turnLeft();
      move();
     turnRight();
      move();
       turnRight();
      move();
      turnLeft();
}
main();
